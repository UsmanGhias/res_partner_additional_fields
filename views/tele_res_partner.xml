<?xml version="1.0" encoding='UTF-8'?>
<odoo>
    <record id="tele_res_partner" model="ir.ui.view">
		<field name="name">res.partner.extended.view </field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page/sheet/group[@class='o_address_format']" position="before">
                <field name="tele_code"/>
            </xpath>
        </field>
	</record>
</odoo>